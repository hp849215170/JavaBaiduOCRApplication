                </AnchorPane>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="583.0" prefWidth="307.0" styleClass="root">
                    <children>
                        <TextArea fx:id="resultText" editable="false" layoutX="-1.0" layoutY="-1.0" prefHeight="585.0" prefWidth="157.0" styleClass="root">
