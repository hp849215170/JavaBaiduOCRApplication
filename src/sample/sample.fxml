    <children>
        <SplitPane fx:id="splitPane" dividerPositions="0.5" nodeOrientation="LEFT_TO_RIGHT" pickOnBounds="false" prefHeight="585.0" prefWidth="800.0">
            <items>
                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="585.0" prefWidth="800.0" styleClass="root">
                    <children>
                        <Button fx:id="upload" layoutX="70.0" layoutY="14.0" mnemonicParsing="false" text="上传图片" AnchorPane.leftAnchor="70.0" AnchorPane.topAnchor="14.0" />
